<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hundsun.demo.springboot.mapper.mysql.TeacherMapper">

    <select id="selectByName" resultType="com.hundsun.demo.springboot.model.domian.TeacherDO">
        select *
        from teacher
        where name = #{name} for
        update
    </select>
</mapper>